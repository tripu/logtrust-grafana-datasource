{
    "name": "devo-grafana-datasource",
    "description": "Devo datasource plugin",
    "version": "1.2.0",
    "license": "MIT",
    "private": true,
    "homepage": "https://github.com/DevoInc/logtrust-grafana-datasource",
    "author": {
        "name": "Devo",
        "url": "https://www.devo.com/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/DevoInc/logtrust-grafana-datasource"
    },
    "bugs": {
        "url": "https://github.com/DevoInc/logtrust-grafana-datasource/issues"
    },
    "devDependencies": {
        "babel": "6.23.0",
        "babel-cli": "6.26.0",
        "babel-plugin-transform-es2015-for-of": "6.23.0",
        "babel-plugin-transform-es2015-modules-systemjs": "6.24.1",
        "babel-preset-es2015": "6.24.1",
        "chai": "4.2.0",
        "jsdom": "15.1.1",
        "mocha": "6.2.0",
        "prunk": "1.3.1",
        "q": "1.5.1"
    },
    "dependencies": {
        "crypto-js": "3.1.9-1",
        "cryptojs": "2.5.3",
        "lodash": "4.17.15",
        "moment": "2.24.0"
    },
    "scripts": {
        "clean": "rm -rf dist/",
        "copy": "mkdir -p dist/ && cp -a src/README.md src/plugin.json src/css/ src/img/ src/partials/ dist/",
        "babel": "babel src/ --out-dir dist/",
        "test": "mocha spec/",
        "build": "npm run clean && npm run copy && npm run babel && npm t"
    },
    "babel": {
        "presets": [
            "es2015"
        ],
        "plugins": [
            "transform-es2015-modules-systemjs"
        ]
    },
    "mocha": {
        "src": [
            "spec/test-main.js",
            "spec/*_spec.js"
        ]
    }
}
